<div class="box">
  <div class="signup-h">
    <h3><%= space.name %></h3>
  </div>
  <div class="backplate">
    <div>
      <img class="spaces-img" src="<%= space.image %>" />
      <div class="fixx">
        <div class="fix"><%= space.name %></div>
        <div class="fix"><%= space.contactName %></div>
        <a href="<%= space.homepage %>" target="_blank">Visit homepage</a>
      </div>
      <div class="button-box">
        <form action="/spaces/<%= space._id %>" method="post">
          <button class="button" type="submit">Request</button>
        </form>
        <a href="/spaces" class="button">Go back</a>
      </div>
    </div>
    <%- include('../partials/notifications') %>
  </div>
</div>