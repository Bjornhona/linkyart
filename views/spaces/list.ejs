<div class="spaces-div overflow">
  <% spaces.forEach(space => { %>
    <div class="spaces">
      <div class="signup-h">
        <h3><%= space.name %></h3>
      </div>
      <div class="backplate">
        <div class="overflow">
          <img class="spaces-img" src="<%= space.image %>" />
          <div><%= space.name %></div>
          <div><%= space.contactName %></div>
          <a href="<%= space.homepage %>" target="_blank">Visit homepage</a>
          <a href="/spaces/<%= space._id %>"><button class="button">See more</button></a>
        </div>
      </div>
    </div>
  <% }) %>
  </div>
</div>