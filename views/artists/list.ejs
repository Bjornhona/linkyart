<h1>Artists</h1>
<div class="spaces-div overflow">
  <% users.forEach(user => { %>
    <div class="spaces">
      <div class="signup-h">
        <h3><%= user.name %></h3>
      </div>
      <div class="backplate">
        <div class="overflow">
          <img class="spaces-img" src="<%= user.image %>" />
          <p><%= user.username %></p>
          <a href="<%= user.homepage %>">Visit homepage</a>
          <a href="/artists/<%= user._id %>"><button class="button">See more</button></a>
        </div>
      </div>
    </div>
  <% }) %>
  </div>
</div>